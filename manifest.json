{
  "manifest_version": 2,
  "name": "Dictionary",
  "description": "Dictionary",
  "version": "0.0.1",
  "background": {
    "scripts": [
      "backgroud.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],

      "js": [
        "content.js"
      ],
      "all_frames": true
    }
  ],
  "browser_action": {},
  "permissions": [
    "tabs"
  ]
}