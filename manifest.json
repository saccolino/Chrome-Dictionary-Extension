{
  "manifest_version": 2,
  "name": "Dictionary",
  "description": "Dictionary",
  "version": "0.1.1",
  "background": {
    "scripts": [
      "js/jquery-3.1.1.min.js",
      "js/backgroud.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],

      "js": [
        "js/jquery-3.1.1.min.js",
        "js/content.js"
      ],
      "css": ["css/content.css"],
       "all_frames": true
    }
  ],
  "web_accessible_resources": ["content.html"],
  "permissions": [
    "tabs"
  ]
}